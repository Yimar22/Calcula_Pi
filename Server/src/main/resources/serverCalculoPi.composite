<?xml version="1.0" encoding="UTF-8"?>

<composite xmlns="http://www.osoa.org/xmlns/sca/1.0"
  xmlns:frascati="http://frascati.ow2.org/xmlns/sca/1.1"
  targetNamespace="http://frascati.ow2.org/helloworld-rmi"
  name="ServerRMI">

  <service name="r" promote="server/r">
   <interface.java interface="java.lang.Runnable" />
  </service>

  <component name="server">

    <property name="serverUri">rmi://localhost:1099/rmiservice</property>

   <service name="r">
      <interface.java interface="java.lang.Runnable" />
    </service>

	<service name="GenerarPts">
		<interface.java interface="org.ow2.frascati.finalproject.calculatepi.annotated.GenerarPts;"></interface.java>
		<frascati:binding.rmi host="localhost" serviceName="rmiservice1" port="1099"/>
	</service>

    	<service name="s">
      	  <frascati:binding.rmi host="localhost" serviceName="rmiservice2" port="2003"/>
    	</service>
 
    <implementation.java class="org.ow2.frascati.finalproject.calculatepi.annotated.Server" />
 
  </component>

</composite>
