<?xml version="1.0" encoding="UTF-8"?>

<composite xmlns="http://www.osoa.org/xmlns/sca/1.0"
  xmlns:frascati="http://frascati.ow2.org/xmlns/sca/1.1"
  targetNamespace="http://frascati.ow2.org/helloworld-rmi"
  name="BrokerRMI">

  <component name="broker">

	<service name="r">
		<interface.java interface="java.lang.Runnable" />
	</service>
 
    <service name="BrokerService">
      <frascati:binding.rmi host="localhost" serviceName="rmiservice" port="1099"/>
	<implementation.java class="package main.java.org.ow2.frascati.finalproject.calculatepi.annotated.BrokerService;" />
    </service>
 
	<service name="BringPoints">
      		<interface.java interface="omain.java.org.ow2.frascati.finalproject.calculatepi.annotated.BringPointsAPI"></interface.java>
      		<frascati:binding.rmi host="localhost" serviceName="rmiservice2" port="2002"/>
    	</service>  

	<implementation.java class="java.org.ow2.frascati.finalproject.calculatepi.annotated.BrokerImpl" />

  </component>

</composite>

